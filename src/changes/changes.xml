<document xmlns="http://maven.apache.org/changes/1.0.0"
		  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		  xsi:schemaLocation="
		http://maven.apache.org/changes/1.0.0 
		http://maven.apache.org/xsd/changes-1.0.0.xsd">

	<properties>
		<title>Martini JMeter Project</title>
		<author email="penny.curich@gmail.com">Penny Rohr Curich</author>
	</properties>

	<body>
		<release version="4.3" date="2019-01-18" description="Release 4.3">
			<action dev="pcurich" type="update">
				Updating Martini core version.
			</action>
		</release>
		<release version="5.0" date="2019-01-18" description="Release 5.0">
			<action dev="pcurich" type="update">
				Updating Martini core version.
			</action>
			<action dev="pcurich" type="update">
				guru.qas.martini.jmeter.controller.MartiniFilterController will use a SecureRandom when shuffle
				is selected but no random seed is provided.
			</action>
			<action dev="pcurich" type="remove">
				Removing StopWatch configuration.
			</action>
		</release>
		<release version="4.2" date="2019-01-02" description="Release 4.2 for Java 11">
			<action dev="pcurich" type="update">
				Updating Variables utility.
			</action>
			<action dev="pcurich" type="add">
				Adding new Spring PreProcessor.
				Adding new Scope Controller.
				Adding new Filter Controller.
				Adding new Bean Sampler.
				Adding new Martini Sampler.
			</action>
			<action dev="pcurich" type="remove">
				Removing directory assembly.
				Removing deprecated code.
			</action>
		</release>
		<release version="4.1.0.1" date="2018-12-08" description="Release 4.1.0.1 for Java 11">
			<action dev="pcurich" type="update">
				Updated to Java 11.
				Updating version dependencies.
			</action>
		</release>
		<release version="4.0.0-JDK10" date="2018-09-04" description="Release 4.0.0 for Java 10">
			<action dev="pcurich" type="update">
				Updated to Java 10.
				Updating version dependencies.
				Updating code to accomodate dependency changes.
				Removed MIT license.
			</action>
		</release>
		<release version="2.6.0" date="2018-08-22" description="Release 2.6.0">
			<action dev="pcurich" type="update">
				Updating version dependencies.
			</action>
		</release>
		<release version="2.5.0" date="2018-08-21" description="Release 2.5.0">
			<action dev="pcurich" type="update">
				Updating version dependencies.
			</action>
		</release>
		<release version="2.4.3" date="2018-07-24" description="Release 2.4.3">
			<action dev="pcurich" type="update">
				Adding current timestamp to failed samples.
				Removing newlines from stacktrace exceptions.
			</action>
		</release>
		<release version="2.4.2" date="2018-06-22" description="Release 2.4.2">
			<action dev="pcurich" type="update">
				Correcting serialization issues.
			</action>
		</release>
		<release version="2.4.1" date="2018-06-06" description="Release 2.4.1">
			<action dev="pcurich" type="add">
				Added debugging statements to SpringBeanUtil.
				No longer terminating run on setup failure; this is up to delegates.
				Correcting serialization issues.
			</action>
		</release>
		<release version="2.4.0" date="2018-04-30" description="Release 2.4.0">
			<action dev="pcurich" type="add">
				New MartiniBeanSampler interfaces/support.
				ForkJoinPool replaced with ExecutorService.
			</action>
		</release>
		<release version="2.3.0" date="2018-04-20" description="Release 2.3.0">
			<action dev="pcurich" type="update">
				Updating martini-core dependency version.
			</action>
		</release>
		<release version="2.2.1" date="2018-04-05" description="Release 2.2.1">
			<action dev="pcurich" type="add">
				Class guru.qas.martini.jmeter.sampler.MartiniSamplerClient now calls
				setupTest(JavaSamplerContext) on its delegate.
			</action>
		</release>
		<release version="2.2.0" date="2018-04-05" description="Release 2.2.0">
			<action dev="pcurich" type="add">
				Class guru.qas.martini.jmeter.Parameterized now returns Optional objects and
				public signature getNormalizedParameter(String) has been removed.
			</action>
		</release>
		<release version="2.1.1" date="2018-04-02" description="Release 2.1.1">
			<action dev="pcurich" type="add">
				Updated assembly to include Martini core.
			</action>
		</release>
	</body>

</document>
